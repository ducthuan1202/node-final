<form class="form-inline" action="" method="GET" style="padding: 20px 0 10px; border: 1px solid #ccc; margin-bottom: 30px; background-color: #e5e5e5;">
    <div class="form-group mx-sm-3 mb-2">
        <input type="text" name="sKeyword" value="<%= searchOptions.sKeyword %>" class="form-control" placeholder="Từ khóa">
    </div>
    <div class="form-group mx-sm-3 mb-2">
        <select name="sStatus" class="custom-select">
            <option value="0"> Select Status </option>
            <% searchOptions.listStatus.forEach(item => { %>
                <option value="<%= item.value %>" <%=(item.value === searchOptions.sStatus) ? "selected" : "" %> >
                    <%= item.label %>
                </option>
            <% }) %>
        </select>
    </div>

    <div class="form-group mx-sm-3 mb-2">
        <button type="submit" class="btn btn-dark mb-2">Search</button>
    </div>
    <div class="form-group mx-sm-3 mb-2">
        <a href="<%= model.getUrlList() %>" class="btn btn-secondary mb-2">Clear</a>
    </div>
</form>